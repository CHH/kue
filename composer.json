{
    "name": "chh/kue",
    "description": "A simple interface to multiple job queue implemenations",
    "authors": [
        {
            "name": "Christoph Hochstrasser",
            "email": "christoph.hochstrasser@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "queue", "jobs", "background processing"
    ],
    "require": {
        "php": ">=5.3.3",
        "evenement/evenement": "~1.0"
    },
    "require-dev": {
        "chh/bob": "~1.0@dev",
        "phpunit/phpunit": "~3.7",
        "symfony/console": "~2.1",
        "monolog/monolog": "~1.2",
        "mtdowling/cron-expression": "~1.0",
        "aws/aws-sdk-php": "~2.1.1"
    },
    "suggest": {
        "ext-redis": "Enable RedisQueue",
        "symfony/console": "Used by the console command",
        "monolog/monolog": "Used by the console command",
        "mtdowling/cron-expression": "Enable CRON expressions",
        "aws/aws-sdk-php": "AmazonSqsQueue"
    },
    "autoload": {
        "psr-0": {
            "Kue\\": "lib/",
            "Kue\\Test\\": "tests/"
        }
    }
}
